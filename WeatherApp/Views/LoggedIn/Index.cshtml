@model IEnumerable<WeatherApp.Models.WeatherModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


    @Html.AntiForgeryToken()

    

    
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateTime)
            </th>
